// Makes the fib series using loops

int main() {
    
    int num,c;
    int a = 0;
    int b = 1;
    
    printf("Enter the num of elements you want");
    scanf("%d",&num);
    printf("1,");
    for(int x = 2; num > x; x++) {
       c = a + b;    
  printf(" %d,",c);    
  a = b;    
  b = c;    
    }
}
