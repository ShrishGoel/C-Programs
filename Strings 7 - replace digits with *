//Program to replace all digits of a string with '*'
int main() {
    char storage[100];
    printf("Enter a string:");
    gets(storage);
    
    for(int x = 0; storage[x] != 0; x++) {
        if(storage[x] >=  48 && storage[x] <= 57) {
            storage[x] = '*';
        }
    }
    printf("The new string is: %s",storage);
}





