// finds the multiplication of a table till 10 entered by the user using loops
int main() {
    
    int n;
    
    printf("Enter the number of which you want the multiplication table:");
    scanf("%d",&n);
    
    for (int x = 1; x <= 10; x++) {
        int total = x * n;
        printf("%d*%d=%d\n",n,x,total);
    
    }
    
    
    
    
}
