// Swaps the number if they are not same using if else

int main() {
    
    float num1,num2;
    
    printf("Enter two numbers:");
    scanf("%f%f",&num1,&num2);
    
    if (num1 != num2) {
    num1 = num1 + num2;
   num2 = num1 - num2;
   num1 = num1 - num2;
   printf("The swapped values are %f and %f",num1,num2);
   
    }
    else {
        printf("The numbers are the same");
    }
}

