// Finds the area of a cylinder using functions

int main() {
    float r,h;
    float vol(float r,float h);
    printf("Enter the radius and height:");
    scanf("%f%f",&r,&h);
    float returna = vol(r,h);
    printf("%f",returna);
}

float vol(float r,float h) {
    float sd = r * r * h * 3.14;
    return sd;
}
