int main() {
    
    for(int x = 5; x >= 1; x--) {
        
        for(int y = 1; y <= x; y++) {
            printf(" ");
        }
        
        for(int z = 5; z > x; z--) {
            
            printf("*");
            if(2) {
                printf("  ");
            }
        }
        printf("\n");
     }
}


